{
  "version": 2,
  "name": "tank-tools-knpc-updated",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "env": {
    "BUILD_TIMESTAMP": "2025-09-27T13:00:00Z"
  }
}
