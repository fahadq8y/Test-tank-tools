{
  "version": 2,
  "name": "tank-tools-knpc",
  "outputDirectory": "./",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/live-tanks",
      "destination": "/live-tanks.html"
    },
    {
      "source": "/plcr",
      "destination": "/plcr.html"
    },
    {
      "source": "/nmogas",
      "destination": "/nmogas.html"
    },
    {
      "source": "/dashboard-test",
      "destination": "/dashboard-test.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}